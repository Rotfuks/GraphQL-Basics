extend type Mutation {
    createOrder(orderInput: OrderInput): Order
}
input OrderInput {
    customerId: ID!
    deliveryadressId: ID
    deliveryadress: AdressInput
    price: Float
}
type Order implements Node {
    id: ID!
    deliveryadress: Adress!
    price: Float
    deliverydate: GraphQLDate
    isDelivered: Boolean
}
